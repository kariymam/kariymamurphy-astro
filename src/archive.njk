---
layout: bloglist.njk
eleventyComputed:
  title: Archive
  description: Showing all blog posts
eleventyNavigation:
  key: archive
  order: 2
numberOfTagsToShow: 10
permalink: /archive/
eleventyImport:
  collections: ["post"]
---
<ol reversed role="list" class="list-none postlist m-6 gap-2" style="counter-reset: start-from {{ (postslistCounter or postslist.length) + 1 }}">
  {% set archive = collections.post  %}
  {% asyncEach post in archive | reverse %}
    {% set articleUrl = post.url %}
    {% set articleDate = post.date %}
    {% set articleTitle = post.data.title %}
    {% set articleDescription = post.data.description %}
    {% set articleImg = post.data.thumb %}
    {% set articleImgAlt = post.data.thumbAlt %}
    {% set start_date = post.data.start_date %}
    {% set end_date = post.data.end_date %}
    {% set articleTags = post.data.tags %}
    {% set articleLink = post.data.externalLink %}
    <!-- {% set currentYear = "" %}
    {% if showYear %}
          {% set postYear = post.date.getFullYear() %}
          {% if currentYear != postYear %}
          {% set currentYear = postYear %}
          <time class="text-xl mx-4 opacity-70 mb-8">{{ postYear }}</time>
          {% endif %}
    {% endif %} -->
    {% include "_partials/card.njk" %}
  {% endeach %}
</ol>